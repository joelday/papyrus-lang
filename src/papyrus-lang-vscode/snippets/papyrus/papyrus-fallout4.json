{
    "Struct": {
        "prefix": "struct",
        "body": [
            "Struct ${1:StructureName}",
            "\t${2:type} ${3:Value}",
            "EndStruct"
        ],
        "description": [
            "Structure Snippet"
        ]
    },
    "Property": {
        "prefix": "prop",
        "body": [
            "${1:type} Property ${2:PropertyName}${0} Auto ${3:Const} ${4:Mandatory}"
        ],
        "description": [
            "Auto Property Snippet"
        ]
    },
    "Property Full": {
        "prefix": "propFull",
        "body": [
            "${1:type} ${2:PropertyName}_var${0}\r\n${1:type} Property ${2:PropertyName} ${3:Hidden}\r\n\t${1:type} Function Get()\r\n\t\treturn ${2:PropertyName}_var\r\n\tEndFunction\r\n\tFunction Set(${1:type} value)\r\n\t\t${2:PropertyName}_var = aValue\r\n\tEndFunction\r\nEndProperty"
        ],
        "description": [
            "Property Full Snippet"
        ]
    },
    "Property Group": {
        "prefix": "group",
        "body": [
            "Group ${1:GroupName}\r\n\t${0}\r\nEndGroup"
        ],
        "description": [
            "Group Snippet"
        ]
    },
    "{Property Group Collapsed": {
        "prefix": "groupCollapsed",
        "body": [
            "Group ${1:GroupName} Collapsed\r\n\t${0}\r\nEndGroup"
        ],
        "description": [
            "Collapsed Group Snippet"
        ]
    }
}