{
    "Script Namespace": {
        "prefix": "script namespace",
        "body": [
            "Scriptname ${1:namespace:}${2:$TM_FILENAME_BASE} extends ${3:ScriptObject}",
            "{${4:The documentation string.}}\n",
            "${0:; code}"
        ],
        "description": [
            "Inserts a script header with namespace.",
            "Only supported by Fallout 4."
        ]
    },
    "Structure": {
        "prefix": "structure",
        "body": [
            "Struct ${1:structurename}",
            "\t${2:type} ${3:Value}${0}",
            "EndStruct"
        ],
        "description": [
            "Inserts a structure.",
            "Only supported by Fallout 4."
        ]
    },
    "Property Constant Mandatory": {
        "prefix": "property auto const mandatory",
        "body": [
            "${1:type} Property ${2:propertyname}${0} Auto ${3:Const} ${4:Mandatory}"
        ],
        "description": [
            "Inserts an auto constant mandatory property.",
            "Only supported by Fallout 4."
        ]
    },
    "Property Group": {
        "prefix": "property group",
        "body": [
            "Group ${1:groupname}${2: Collapsed}\r",
            "\t${0}\r",
            "EndGroup"
        ],
        "description": [
            "Inserts a property group.",
            "Only supported by Fallout 4."
        ]
    },
    "State (Fallout 4)": {
        "prefix": "state (Fallout 4)",
        "body": [
            "${1:Auto }State ${2:statename}${0}\r",
            "\t\t; Note: Parameterized state events are only supported in Fallout 4.\r",
            "\tEvent OnBeginState(string oldState)",
            "\t\t${3:; code}\r",
            "\tEndEvent",
            "\tEvent OnEndState(string newState)",
            "\t\t${4:; code}\r",
            "\tEndEvent",
            "EndState"
        ],
        "description": [
            "Inserts a state.",
            "Only supported by Fallout 4."
        ]
    }
}